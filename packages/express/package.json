{
  "name": "@library-api/express",
  "version": "0.1.0",
  "description": "An Express API for easily accessing information related to meetings and publications of Jehovah's Witnesses.",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "ci": "yarn lint && jest --bail",
    "clean": "rm -rf dist",
    "dev": "tsc-watch --build --incremental --onSuccess \"yarn start\"",
    "lint": "tsc --noEmit && eslint . --ext .js,.ts",
    "start": "node dist/server.js",
    "test": "jest --coverage -t",
    "test:watch": "jest --watch-all -t",
    "husky:lint": "yarn lint --fix",
    "husky:test": "jest --bail",
    "husky:test:staged": "jest --bail --findRelatedTests"
  },
  "dependencies": {
    "@library-api/core": "0.1.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.11"
  },
  "author": {
    "name": "BenShelton",
    "email": "bensheltonjones@gmail.com"
  },
  "keywords": [
    "jw",
    "media",
    "library",
    "meetings",
    "publications",
    "api",
    "express"
  ],
  "license": "MIT"
}
