{
  "name": "@library-api/express",
  "version": "0.1.0",
  "description": "An Express API for easily accessing information related to meetings and publications of Jehovah's Witnesses.",
  "main": "dist/server.js",
  "types": "dist/server.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist",
    "dev": "ts-node-dev --respawn -T -P ./tsconfig.json -r tsconfig-paths/register src/server.ts",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "start": "node dist/server.js",
    "test": "jest --coverage -t",
    "test:watch": "jest --watch-all -t",
    "husky:lint": "yarn lint",
    "husky:test": "jest --bail --findRelatedTests"
  },
  "keywords": [
    "jw",
    "media",
    "library",
    "meetings",
    "publications",
    "api",
    "express"
  ],
  "dependencies": {
    "@library-api/core": "0.1.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/node-fetch": "^2.5.8",
    "@types/unzipper": "^0.10.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "tsconfig-paths": "^3.9.0"
  },
  "license": "MIT"
}
