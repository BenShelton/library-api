{
  "name": "@library-api/core",
  "version": "0.1.0",
  "description": "Core tools for use with @library-api packages.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "ci": "yarn lint && jest --bail",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit && eslint . --ext .js,.ts",
    "test": "jest --coverage -t",
    "test:watch": "jest --watch-all -t",
    "husky:lint": "yarn lint --fix",
    "husky:test": "jest --bail",
    "husky:test:staged": "jest --bail --findRelatedTests"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "sqlite": "^4.0.19",
    "sqlite3": "^5.0.2",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.10",
    "@types/sqlite3": "^3.1.7",
    "@types/unzipper": "^0.10.3"
  },
  "author": {
    "name": "BenShelton",
    "email": "bensheltonjones@gmail.com"
  },
  "keywords": [
    "jw",
    "media",
    "library",
    "meetings",
    "publications",
    "api",
    "core"
  ],
  "license": "MIT"
}
