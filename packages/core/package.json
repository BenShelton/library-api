{
  "name": "@library-api/core",
  "version": "0.3.1",
  "description": "Core tools for use with @library-api packages.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BenShelton/library-api",
    "directory": "packages/core"
  },
  "scripts": {
    "build": "tsc --build tsconfig.json",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "yarn lint --fix",
    "test": "jest --config=jest.config.ts",
    "test:bail": "yarn test --bail",
    "test:coverage": "yarn test --coverage",
    "test:ci": "yarn lint && yarn tsc && yarn test:bail",
    "test:watch": "yarn test --watch-all -t",
    "test:staged": "yarn test --bail --findRelatedTests",
    "tsc": "tsc --noEmit"
  },
  "dependencies": {
    "node-fetch": "^2.6.1",
    "sqlite": "^4.0.19",
    "sqlite3": "^5.0.2",
    "unzipper": "^0.10.11"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.5.10",
    "@types/sqlite3": "^3.1.7",
    "@types/unzipper": "^0.10.3"
  },
  "author": {
    "name": "BenShelton",
    "email": "bensheltonjones@gmail.com"
  },
  "keywords": [
    "jw",
    "media",
    "library",
    "meetings",
    "publications",
    "api",
    "core"
  ],
  "license": "MIT"
}
