{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "composite": false,
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable"
    ]
  },
  "include": [
    "src/**/*.vue",
    "src/**/*.ts",
    "types/**/*.d.ts",
    "../preload/types/electron-api.d.ts",
    "../../types/**/*.d.ts"
  ]
}
