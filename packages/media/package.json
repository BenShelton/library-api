    "@vue/compiler-sfc": "^3.0.11",
    "chokidar": "^3.5.1",
    "electron": "^12.0.2",
    "electron-builder": "^22.10.5",
    "eslint-plugin-vue": "^7.8.0",
    "vite": "^2.1.5"
  },
  "author": {
    "name": "BenShelton",
    "email": "bensheltonjones@gmail.com"
  },
  "keywords": [
